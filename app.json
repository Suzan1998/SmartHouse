{
  "expo": {
    "platforms": [
      "ios",
      "android",
      "web"],
      "ios": {
        "bundleIdentifier": "com.suzan.SmartHome",
        "buildNumber": "1.1.0",
        "supportsTablet": true
      },
      "android": {
        "package": "com.suzan.SmartHome",
        "versionCode": 1,
        "useNextNotificationsApi": true
      },
    
    "name": "home",
